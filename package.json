{
  "name": "service-template",
  "version": "1.0.0",
  "description": "Service template for Dokan SAAS",
  "main": "dist/index.js",
  "author": "weDevs",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest --detectOpenHandles",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/index.js",
    "fmt": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "yarn run eslint --ext .ts src/",
    "postinstall": "cp -n .env.example .env || true",
    "route:list": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/bin/route-list.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.2.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.27.2",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "mongodb-memory-server": "^8.3.0",
    "mongoose": "^6.1.4",
    "mongoose-slug-generator": "^1.0.4",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "tsconfig-paths": "^4.0.0",
    "typedi": "^0.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.4"
  }
}
